{
  "name": "price",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.4",
    "@types/swagger-ui-dist": "^3.30.0",
    "@types/swagger-ui-express": "^4.1.2",
    "swagger-ui-express": "^4.1.4",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslint": "^6.1.3",
    "tsoa": "^3.2.1",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "dev": "tsnd app/app.ts --respawn --transpileOnly",
    "dev:doc": "tsnd documentation/documentation.ts --respawn --transpileOnly",
    "build:ts": "tsc --resolveJsonModule",
    "build:routes": "tsoa app/routes -c app/config/tsoa.json",
    "build:doc": "mkdir -p api && mkdir -p api/dist && tsoa swagger -c app/config/tsoa.json",
    "build:all": "npm run build:routes && npm run build:doc && npm run build:ts",
    "lint": "tslint -c app/config/tslint.json 'src/**/*.ts'",
    "server": "node ./build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kloktig/price.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kloktig/price/issues"
  },
  "homepage": "https://github.com/kloktig/price#readme"
}
